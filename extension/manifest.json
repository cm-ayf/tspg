{
	"browser_action": {
		"browser_style": true,
		"default_title": "tspg",
		"default_popup": "https://tspg.cc/extension",
		"default_icon": {}
	},
	"description": "TypeScript Playground URL shortening browser extension",
	"icons": {},
	"homepage_url": "https://github.com/cm-ayf/tspg",
	"manifest_version": 2,
	"name": "tspg",
	"version": "1.0",
	"permissions": ["tabs", "webNavigation"],
	"content_security_policy": "default-src *; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; connect-src tspg.cc;",
	"commands": {
		"save": {
			"suggested_key": {
				"default": "Ctrl+Shift+S"
			},
			"description": "Save your code"
		}
	}
}
