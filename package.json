{
	"name": "tspg.cc",
	"version": "0.1.1",
	"homepage": "https://tspg.cc/",
	"description": "Short URL Generator for TypeScript Playground",
	"repository": {
		"type": "git",
		"url": "https://github.com/cm-ayf/tspg.git"
	},
	"bugs": {
		"url": "https://github.com/cm-ayf/tspg/issues"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "web-ext build --source-dir .svelte-kit/extension --artifacts-dir .svelte-kit/extension/dist --filename tspg.zip --overwrite-dest",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^2.3.1",
		"@sveltejs/kit": "^1.22.4",
		"@types/node": "^20.4.5",
		"@types/webextension-polyfill": "^0.10.1",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-svelte": "^2.32.4",
		"lint-staged": "^13.2.3",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.3",
		"simple-git-hooks": "^2.9.0",
		"svelte": "^4.1.2",
		"svelte-check": "^3.4.3",
		"tslib": "^2.6.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.8",
		"web-ext": "^7.6.2",
		"webextension-polyfill": "^0.10.0"
	},
	"type": "module",
	"dependencies": {
		"@octokit/core": "^5.0.0",
		"lz-string": "^1.5.0"
	}
}
